#include <iostream>

using namespace std;

class Rectangle {
    private:
        int width, height;
    public:
        Rectangle(int w, int h) {
            width = w;
            height = h;
        }

        int getArea() {
            return width * height;
        }

        void printInfo() {
            cout << "Width: " << width << endl;
            cout << "Height: " << height << endl;
            cout << "Area: " << getArea() << endl;
        }
};

int main() {
    Rectangle rect(10, 5);
    rect.printInfo();

    return 0;
}
